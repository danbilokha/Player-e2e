{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@store/*": ["src/app/store/*"],
      "@store": ["src/app/store/index"],
      "@features/*": ["src/app/features/*"],
      "@features": ["src/app/features/index"],
      "@shared/*": ["src/app/shared/*"],
      "@shared": ["src/app/shared/index"],
      "@api-connectors/*": ["src/app/api-connectors/*"],
      "@api-connectors": ["src/app/api-connectors/index"],
      "@testing/*": ["src/app/testing/*"]
    },
    "outDir": "../out-tsc/app",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "incremental": true,
    "tsBuildInfoFile": "./buildcache/front-end",
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
